<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="controller.LoginController">
   <center>
      <Pane prefHeight="300.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="tfUser" layoutX="67.0" layoutY="114.0" prefHeight="24.0" prefWidth="166.0" />
            <PasswordField fx:id="tpPassword" layoutX="69.0" layoutY="187.0" prefHeight="25.0" prefWidth="163.0" />
            <Label layoutX="109.0" layoutY="52.0" prefHeight="28.0" prefWidth="83.0" text="Cine ICESI">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="btLogin" layoutX="112.0" layoutY="233.0" mnemonicParsing="false" onAction="#logIn" prefHeight="34.0" prefWidth="72.0" text="Login">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </center>
</BorderPane>
